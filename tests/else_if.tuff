// Else-if chain test

fn classify(x: I32) -> I32 {
    if (x < 0) {
        return 0;
    } else if (x == 0) {
        return 1;
    } else if (x < 10) {
        return 2;
    } else if (x < 100) {
        return 3;
    } else {
        return 4;
    }
}

fn main() -> I32 {
    let a: I32 = classify(-5);  // 0
    let b: I32 = classify(0);   // 1
    let c: I32 = classify(5);   // 2
    let d: I32 = classify(50);  // 3
    let e: I32 = classify(500); // 4
    
    return a + b + c + d + e;  // 10
}
