fn max(a : I32, b : I32) : I32 => {
    if (a > b) {
        return a;
    } else {
        return b;
    }
}

fn main() : Void => {
    let x : I32 = max(10, 20);
    printf("Max of 10 and 20 is %d\n", x);
    
    if (x > 15) {
        printf("x is greater than 15\n");
    } else {
        printf("x is less than or equal to 15\n");
    }
}
