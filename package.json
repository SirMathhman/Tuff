{
  "name": "tuff-interpret",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "cpd": "pmd cpd -d src,tests -l typescript --minimum-tokens 50 -f text",
    "cpd:ignore": "pmd cpd -d src,tests -l typescript --minimum-tokens 50 -f text --ignore-identifiers --ignore-literals"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "jest": "^29.6.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.2"
  }
}
